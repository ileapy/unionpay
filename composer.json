{
    "name": "cfn/unionpay",
    "description": "银联云闪付小程序非官方SDK，小程序后端与支付相关接口已更新完毕。",
    "type": "library",
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "symfony/cache": "^3.3 || ^4.3 || ^5.0 || ^6.0",
        "symfony/http-foundation": "^2.7 || ^3.0 || ^4.0 || ^5.0 || ^6.0",
        "pimple/pimple": "^3.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-zlib": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.5 || ^7.0 || ^8.0 || ^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "cfn",
            "email": "cfn@leapy.cn"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "unionpay\\": "src/",
            "unionpay\\Tests\\": "tests/"
        }
    }
}
